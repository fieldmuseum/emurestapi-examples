{
    "name": "emurestapi/examples",
    "description": "examples for Axiell's EMu REST API, in PHP",
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.9",
        "psr/log": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "autoload": {
        "psr-4": {"EMuRestApi\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
